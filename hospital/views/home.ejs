    <script>
        $(document).ready(function() {
    // Reference to the input field and suggestions container
    const searchInput = $('#searchInput');
    const suggestionsContainer = $('#main-container-for-suggestions');

    // Function to perform a search based on the selected suggestion
    const performSearch = (selectedSuggestion) => {
        // Update the input field with the selected suggestion
        searchInput.val(selectedSuggestion.name);

        // Perform the search
        window.location.href = `/search?q=${selectedSuggestion.name}`;
    };

    // Event listener for input changes
    searchInput.on('input', function() {
        const query = searchInput.val();

        // Make an AJAX request to your server to get suggestions
        $.ajax({
            url: `/suggest?q=${query}`,
            method: 'GET',
            success: function(response) {
                // Update the suggestions container with the received suggestions
                suggestionsContainer.html('');
                response.forEach(suggestion => {
                    const suggestionElement = $(`<div>${suggestion.name}</div>`);


                    suggestionElement.css({
                        backgroundColor: '#fff',
                        padding: '10px',
                        border: '1px solid #ccc',
                        borderRadius: '5px',
                        margin: '5px',
                        fontSize: "20px",
                        color: "#000",
                        cursor: 'pointer', // Add a pointer cursor for better interaction
                        transition: 'background-color 0.3s ease', // Smooth transition effect
                    });
                    //Add hover effect
                        suggestionElement.hover(
                            function () {
                                $(this).css('background-color', '#007C9D');
                            },
                            function () {
                                $(this).css('color', '#f9f9f9');
                            },
                            function () {
                                $(this).css('textDecoration', 'underline');
                            }
                        );
                    // Attach a click event to the suggestion to perform the search
                    suggestionElement.click(() => performSearch(suggestion));

                    suggestionsContainer.append(suggestionElement);
                });
            },
            error: function(error) {
                console.error('Error fetching suggestions:', error);
            }
            });
        });
    });
    </script>
    
